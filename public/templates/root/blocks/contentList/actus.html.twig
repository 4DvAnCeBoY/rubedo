<div id="list-{{ prefix }}-{{ page.currentPage }}">

    <ul class="nav nav-tabs nav-stacked active">
    	{% for content in data %}
    	<li><a href="{{ displaySingleUrl(content.id) }}">
    	    <div class="pull-left">
    	        {% if content.image %}
    	        <img alt=""
    			class="img-rounded"
    			src="/dam?media-id={{content.image}}&width=100&height=60&mode=crop" />
    			{% endif %}
    			</div>
    	       <div style="padding-left: 8em;">{{ content.title }}
    			    
    			</div> 
    			<div class="clearfix"></div>
    			</a>
    			
    			</li> {% endfor %}
    </ul>
</div>