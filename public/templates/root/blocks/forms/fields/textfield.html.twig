
<div class="control-group {% if field.id in errors|keys %}error{% endif %} {% if field.itemConfig.hidden %}hide{% endif %}" >
    <label class="control-label" for="{{ field.id }}">{{ field.itemConfig.label }}</label>
       <a href="#" class="help" data-toggle="tooltip"  data-placement="right"  title="{{ field.itemConfig.tooltip }}" data-original-title="{{ field.itemConfig.tooltip }}">
      <i class="icon-exclamation-sign" style="margin-left:10px;"></i></a>
    <div class="controls">
      <input class="{% if field.itemConfig.conditionalQuestion %}conditional{% endif %}"  {% if field.itemConfig.target %}data-target="{{field.itemConfig.target}}"{% endif %} {% if field.itemConfig.value %}data-value="{{field.itemConfig.value}}"{% endif %}  type="text" id="{{ field.id }}" name="{{ field.id }}" value="{{ values[field.id] }}">
      {% if field.id in errors|keys %}<span class="help-inline">{{ errors[field.id] }}</span>{% endif %}
   
    </div>
  </div>