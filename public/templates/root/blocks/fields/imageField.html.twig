 {% if(cValue["config"]["multivalued"])and value is iterable %}
  {% for item in value %}
<div class="pull-left thumbnail editable-img thumbnail-rubedo" id="{{ data.id }}_{{ key }}-{{ loop.index-1 }}">
    <img class="image-field img-rounded imageLeft" src="/dam?media-id={{ item }}" alt="{{data.text}}" title="{{data.text}}" />
</div>
{% endfor %} 
{% else %}
<div class="pull-left thumbnail editable-img thumbnail-rubedo" id="{{ data.id }}_{{ key }}">
    <img class="img-field img-rounded imageLeft" src="/dam?media-id={{ value }}" alt="{{data.text}}" title="{{data.text}}" />
</div>
{% endif %}
