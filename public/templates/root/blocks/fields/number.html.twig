{%  if(cValue["config"]["multivalued"]) and value is iterable%}
	{% for item in value %}
		<div class="number" id="{{ data.id }}_{{ key }}-{{ loop.index-1 }}">
    <span class="currentNumber">{{ item }}</span>
</div>
	{% endfor %}
{% else %}
	<div class="number" id="{{ data.id }}_{{ key }}">
    <span class="currentNumber">{{ value }}</span>
</div>
{% endif %}