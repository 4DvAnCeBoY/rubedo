{%  if(cValue["config"]["multivalued"]) %}
	{% for item in value %}
		<div><span class="editable" id="{{ data.id }}_{{key}}#{{ loop.index-1 }}">{{ item|cleanHtml}}</span></div>
	{% endfor %}
{% else %}
	<div><span class="editable" id="{{ data.id }}_{{key}}">{{ value |cleanHtml }}</span></div>
{% endif %}