{% extends "root/page.html.twig" %}

    
    {% block pageStyle %}
    <!-- Le styles -->
    <link href="{{baseUrl}}/components/thomaspark/bootswatch/united/bootstrap.min.css" rel="stylesheet">
    <link href="{{baseUrl}}/components/twitter/bootstrap/css/bootstrap-responsive.min.css" rel="stylesheet">
    <link href="{{baseUrl}}/components/zencoder/video-js/video-js.min.css" rel="stylesheet">
    
    
    {% for sheet in css %}
    <link href="{{baseUrl}}{{sheet}}" rel="stylesheet" />
    {% endfor %}
    
    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    		<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    		<![endif]-->
    {% endblock %} 
    
    
	{% block pageJavascript %}
    	<!-- Le javascript
    		================================================== -->
    	<!-- Placed at the end of the document so the pages load faster -->
    	<script src="{{baseUrl}}/components/jquery/jquery/jquery-1.9.1.min.js"></script>
    	<script src="{{baseUrl}}/components/twitter/bootstrap/js/bootstrap.min.js"></script>
      	<script src="{{baseUrl}}/components/zencoder/video-js/video.min.js"></script>
      	<script>
        	_V_.options.flash.swf = "{{baseUrl}}/components/zendcoder/video-js/video-js.swf";
      	</script>
    	
    	{% for file in js %}
    	<script type="text/javascript" src="{{baseUrl}}{{file}}"></script>
    	{% endfor %}
	{% endblock %}