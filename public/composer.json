{
    "name": "webtales/rubedo-public",
    "require": {
    	"webtales/rubedo-backoffice-ui" : "dev-master",
        "jquery/jquery" : "1.9.1",
        "sencha/extjs" : "4.1.1a",
        "webtales/ckeditor" : "dev-master",
        "jackmoore/colorbox" : "1.4.2",
        "twitter/bootstrap" : "v2.3.0",
        "zencoder/video-js" : "3.2.0"
    },
    "require-dev": {
        
    },
    "repositories": [
		{
        "type": "package",
        "package": {
	            "name": "jquery/jquery",
	            "version": "1.9.1",
	            "dist": {
	                "url": "http://code.jquery.com/jquery-1.9.1.min.js",
	                "type": "file"
	            }
	        }
	    },        
        {
        "type": "package",
        "package": {
            "name": "sencha/extjs",
            "version": "4.1.1a",
            "dist": {
                "url": "http://cdn.sencha.com/ext-4.1.1a-gpl.zip",
                "type": "zip"
            	}
        	}
        
    	},
    	{
        "type": "package",
        "package": {
            "name": "ckeditor/ckeditor",
            "version": "4.0.1",
            "dist": {
                "url": "http://download.cksource.com/CKEditor/CKEditor/CKEditor%204.0.1/ckeditor_4.0.1_full.zip",
                "type": "zip"
            	}
        	}
        
    	},
    	{
        "type": "package",
        "package": {
            "name": "jackmoore/colorbox",
            "version": "1.4.2",
            "dist": {
                "url": "https://github.com/jackmoore/colorbox/archive/1.4.2.zip",
                "type": "zip"
            	}
        	}
        
    	},
    	{
        "type": "package",
        "package": {
            "name": "twitter/bootstrap",
            "version": "v2.3.0",
            "dist": {
                "url": "http://twitter.github.com/bootstrap/assets/bootstrap.zip",
                "type": "zip"
            	}
        	}
        
    	},
    	{
        "type": "package",
        "package": {
            "name": "zencoder/video-js",
            "version": "3.2.0",
            "dist": {
                "url": "https://github.com/downloads/zencoder/video-js/video-js-3.2.0.zip",
                "type": "zip"
            	}
        	}
        
    	},
    	{
			"type": "vcs",
			"url": "git://github.com/WebTales/rubedo-backoffice-ui.git"
		},
		{
			"type": "vcs",
			"url": "git://github.com/WebTales/ckeditor.git"
		}
	],
    "minimum-stability": "stable",
    "config": {
        "process-timeout": 600,
        "vendor-dir" : "components"
    }
}

