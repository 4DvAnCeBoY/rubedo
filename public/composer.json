{
	"name": "webtales/rubedo-public",
	"require": {
		"jquery/jquery": "1.10.0",
		"jquery/jqueryui": "1.10.2",
		"jquery/timepicker": "0.3.2",
		"sencha/extjs": "4.1.1a",
		"webtales/ckeditor": "dev-develop",
		"twitter/bootstrap": "v2.3.0",
		"thomaspark/bootswatch" : "v2.3.0",
		"webtales/rubedo-backoffice-ui": "dev-develop",
		"longtailvideo/jwplayer": "6",
		"webtales/rubedo-localization": "dev-master",
		"jschr/bootstrap-modal": "2.1"
	},
	"require-dev": {},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "jquery/jquery",
				"version": "1.10.0",
				"dist": {
					"url": "http://code.jquery.com/jquery-1.10.0.min.js",
					"type": "file"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jquery/jqueryui",
				"version": "1.10.2",
				"dist": {
					"url": "http://jqueryui.com/resources/download/jquery-ui-1.10.2.zip",
					"type": "zip"
				}
			}
		},
		 {
			"type": "package",
			"package": {
				"name": "sencha/extjs",
				"version": "4.1.1a",
				"dist": {
					"url": "http://cdn.sencha.com/ext-4.1.1a-gpl.zip",
					"type": "zip"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "twitter/bootstrap",
				"version": "v2.3.0",
				"dist": {
					"url": "http://twitter.github.com/bootstrap/assets/bootstrap.zip",
					"type": "zip"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "thomaspark/bootswatch",
				"version": "v2.3.0",
				"dist": {
					"url": "https://github.com/thomaspark/bootswatch/archive/v2.3.0+1.zip",
					"type": "zip"
				}
			}
		},
		 {
			"type": "package",
			"package": {
				"name": "longtailvideo/jwplayer",
				"version": "6",
				"dist": {
					"url": "https://github.com/WebTales/jwplayer/archive/6.zip",
					"type": "zip"
				}
			}
		},
		 {
			"type": "package",
			"package": {
				"name": "jquery/timepicker",
				"version": "0.3.2",
				"dist": {
					"url": "http://fgelinas.com/code/timepicker/releases/jquery-ui-timepicker-0.3.2.zip",
					"type": "zip"
				}
			}
		},
		
		
		 {
			"type": "vcs",
			"url": "git://github.com/WebTales/rubedo-backoffice-ui.git"
		}, {
			"type": "vcs",
			"url": "git://github.com/WebTales/ckeditor.git"
		},{
			"type": "vcs",
			"url": "git://github.com/WebTales/rubedo-localization.git"
		},{
		    "type": "vcs",
		    "url": "git://github.com/jschr/bootstrap-modal.git"
		}
	],
	"minimum-stability": "stable",
	"config": {
		"process-timeout": 600,
		"vendor-dir": "components"
	}
}