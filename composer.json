{
	"name": "webtales/rubedo",
	"require": {
		"php": ">=5.3.0",
		"ext-fileinfo": "*",
		"ext-gd": "*",
		"ext-mongo": ">=1.3.0",
		"twig/twig": "1.13.*",
		"webtales/mongo-filters": "1.4.0",
		"twig/extensions": "1.0.*",
		"monolog/monolog": "1.6.*",
		"ruflin/Elastica": "0.90.*",
		"ezyang/htmlpurifier": "4.5.*",
		"zendframework/zendframework": "2.*",
		"zendframework/zendservice-flickr":"dev-master",
		"alb/oembed": "1.1.*",
		"swiftmailer/swiftmailer": "5.0.*",
		"phing/phing": "2.5.0",
		"leafo/lessphp": "v0.4.0",
		"mrclay/minify": "2.1.7",
		"zendframework/zendservice-recaptcha": "2.0.1"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"chriskite/phactory": "0.4.*",
		"phpmd/phpmd": "1.4.*",
		"squizlabs/php_codesniffer": "1.4.*"
	},
	"minimum-stability": "stable",
	"config": {
		"process-timeout": 600
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git://github.com/WebTales/MongoFilters.git"
		}
	],
	"autoload": {
        "classmap": ["module/Rubedo/src/Rubedo"]
    }
}
